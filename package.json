{
  "name": "intranet-cdt",
  "version": "1.3.4",
  "description": "Intranet du Carreau du Temple",
  "main": "app/main.js",
  "scripts": {
    "prestart": "rimraf out && electron-forge package && electron-forge make",
    "start": "echo 'done!'"
  },
  "keywords": [],
  "author": "Le Carreau du Temple",
  "license": "ISC",
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-squirrel-startup": "^1.0.0",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "electron": "^22.0.0",
    "electron-winstaller": "^5.1.0",
    "rimraf": "^3.0.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "intranet-cdt",
        "icon": "./assets/icons/icon.ico",
        "platform": "win32",
        "arch": "x64",
        "outDir": "release-builds"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "intranet-cdt",
            "authors": "Le Carreau du Temple",
            "description": "Intranet du Carreau du Temple",
            "icon": "./assets/icons/icon.ico"
          }
        }
      ],
      "plugins": []
    }
  }
}
